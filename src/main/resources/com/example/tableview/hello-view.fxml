<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<BorderPane prefHeight="468.0" prefWidth="249.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tableview.HelloController">
   <center>
      <TableView fx:id="tableView" prefHeight="359.0" prefWidth="249.0">
           <columns>
               <TableColumn fx:id="cate_id" prefWidth="120.0" text="ID" />
               <TableColumn fx:id="cate_name" prefWidth="127.0" text="Category Name" />
           </columns>
       </TableView>
   </center>
   <bottom>
      <VBox prefHeight="67.0" prefWidth="249.0" BorderPane.alignment="CENTER">
         <children>
            <HBox>
               <children>
                  <Button mnemonicParsing="false" onAction="#movetoAdd" text="Adding new data" />
                  <Button fx:id="btnReload" mnemonicParsing="false" text="Reload">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <padding>
                  <Insets left="30.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </bottom>
</BorderPane>
